@model IEnumerable<SieuThi.Models.Home.MatHangListModel>
<div class="p-5 mb-4 bg-light rounded-3">
    <div class="container-fluid py-5">
        <h1 class="display-5 fw-bold">@ViewBag.Title2</h1>
        <p class="col-md-8 fs-4">Đây là trang demo connect & get Data từ SQL Server.</p>
        <button id="search" class="btn btn-primary btn-lg" type="button">Thử get dữ liệu</button>
    </div>
</div>
<hr />
<div id="searchResults">
    <!--Data-->
    @Html.Partial("_PartialMatHangList", Model)
</div>
<script>
    // A $( document ).ready() block.
    $(document).ready(function () {
        var url = '@Url.Action("GetMatHangList", "Home")';
$('#search').click(function() {
    var keyWord = "";
    alert("Search Thành CÔng!");
  $('#searchResults').load(url, { searchText: keyWord });
})
    });
</script>


